    "commander": "^6.1.0",
    "cross-fetch": "^3.1.5",
    "fs-extra": "10.0.1",
    "handlebars": "^4.7.3",
    "pgtools": "^0.3.0",
    "puppeteer": "^13.1.1",
    "tree-kill": "^1.2.2",
    "ts-node": "^10.0.0"
